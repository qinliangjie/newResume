.edit_list{
  width: 90%;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  border-bottom:1px solid rgba(0, 0, 0, .1);
  margin-bottom: 50rpx;
  padding-bottom: 5rpx
}
.edit_list .left_part{
  font-size: 15px;
  color: #666;
  width: 170rpx;
}
.edit_list .right_part{
  font-size: 15px;
  color: #333;
  flex-grow:1;
}

.edit_list .right_part .picker{
  width:300rpx;
  height:45rpx;
}